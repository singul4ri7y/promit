// 'String::stringify(value)' converts any value to it's string
// representation.

const class A { __represent__() { return NaN } };

showl String::stringify(101325);
showl String::stringify(System());
showl String::stringify(A());

showl typeof String::stringify(A());

// See the output for yourself.
