// Dictionary(dict) constructor creates identical but new dictionary
// within the instance.

take dict = { a : 10, b : 20, c : 30 };

showl dict;    // Output : { c : 30, b : 20, a : 10 };

take instance_dict = Dictionary(dict);

showl instance_dict;    // Output : { c : 30, b : 20, a : 10 }

showl dict == instance_dict.value();    // Output : false
