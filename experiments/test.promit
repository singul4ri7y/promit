// Variable declaration.

take a = 124458;
take b = null;
take c = 15;

showl 'The result of a % b is: ${a % b}';
showl 'The result of a % c is: ${a % c}';
showl 'The result of a / b is: ${a / b}';
showl 'The result of b * c is: ${b * c}';
showl 'The result of a * c is: ${a * c}';
